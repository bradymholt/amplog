{
  "name": "xertz",
  "version": "0.51.0",
  "description": "A static site generator written in TypeScript",
  "author": "Brady Holt",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "bin": "bin/start.js",
  "files": [
    "bin/**/*.js",
    "dist/",
    "scaffold/"
  ],
  "scripts": {
    "build_scaffold": "cd scaffold/ && npx ts-node --files ../bin/start.ts build",
    "test": "npx jest",
    "prepublishOnly": "tsc"
  },
  "dependencies": {
    "@bradymholt/ampify": "^1.2.0",
    "@types/package-json": "^5.0.1",
    "chokidar": "^3.3.1",
    "dateformat": "^3.0.3",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "gray-matter": "^4.0.2",
    "handlebars": "^4.7.3",
    "js-yaml": "^3.13.1",
    "marked": "^0.8.0",
    "node-sass": "^4.13.1",
    "npm-check-updates": "^4.0.4",
    "package-json": "^6.5.0",
    "prismjs": "^1.19.0"
  },
  "devDependencies": {
    "@types/dateformat": "^3.0.1",
    "@types/express": "^4.17.3",
    "@types/fs-extra": "^8.1.0",
    "@types/jest": "^25.1.4",
    "@types/js-yaml": "^3.12.2",
    "@types/marked": "^0.7.3",
    "@types/node": "^13.9.1",
    "@types/node-sass": "^4.11.0",
    "@types/prismjs": "^1.16.0",
    "jest": "^25.1.0",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  }
}
