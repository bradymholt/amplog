{
  "name": "xertz",
  "version": "0.50.0",
  "description": "A static site generator written in TypeScript",
  "author": "Brady Holt",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "bin": "bin/start.js",
  "files": [
    "bin/**/*.js",
    "dist/",
    "scaffold/"
  ],
  "scripts": {
    "build_scaffold": "cd scaffold/ && npx ts-node --files ../bin/start.ts build",
    "test": "npx jest",
    "prepublishOnly": "tsc"
  },
  "dependencies": {
    "@bradymholt/ampify": "^1.2.0",
    "@types/package-json": "^5.0.1",
    "chokidar": "^3.1.1",
    "dateformat": "^3.0.3",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "gray-matter": "^4.0.2",
    "handlebars": "^4.3.1",
    "js-yaml": "^3.13.1",
    "marked": "^0.7.0",
    "node-sass": "^4.12.0",
    "npm-check-updates": "^3.1.23",
    "package-json": "^6.5.0",
    "prismjs": "^1.17.1"
  },
  "devDependencies": {
    "@types/dateformat": "^3.0.1",
    "@types/express": "^4.17.1",
    "@types/fs-extra": "^8.0.0",
    "@types/jest": "^24.0.18",
    "@types/js-yaml": "^3.12.1",
    "@types/marked": "^0.6.5",
    "@types/node": "^12.7.8",
    "@types/node-sass": "^4.11.0",
    "@types/prismjs": "^1.16.0",
    "jest": "^24.9.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  }
}
